<header class="indexhead">
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="">
    <div class="navbar-header">
           
    <span id="pagename" class="navbar-brand font-fam">Discover</span>
      
           <ul class="font-fam solve  pull-right">
            
           <li>
           <%= link_to search_path, class: "hov", :"data-no-turbolink" => true do %>	
          <span class="headglyph glyphicon glyphicon-eye-open"></span>
          <% end %> 
            </li>
     
            <li>
          <%= link_to edit_user_path(current_user), class: "hov", :"data-no-turbolink" => true do %>
          <span class="headglyph glyphicon glyphicon-edit"></span>
          <% end %>    
            </li>
            
           <li> 
         <%= link_to signout_path, class: "hov", method: "delete" do %>
         <span class="headglyph glyphicon glyphicon-off"></span>
          <% end %>        
            </li>
      </ul>
        </div>   
      </div><!-- /.container -->
    </nav>
</header>

 <div class="container micropostindex"> 
 	
  <% @microposts.each do |micropost| %>
  
  <div class="micropostindex2"> <%= link_to (image_tag micropost.photo.url(:square), class: "img-responsive"), micropost, :"data-no-turbolink" => true %></div>
  
  <% end %>
  
 </div>
 
 <div class="pager">
   <%= will_paginate @microposts, :page_links => false %>
 </div>

 <div class="footer2"><%= render 'layouts/minfooter' %></div>