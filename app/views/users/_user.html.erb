<span class="font-fam">
<div class="userpartialimg"><%= image_tag user.avatar.url(:thumb), class: "img-circle" %></div>
<ul class="ulpartial">
 <li>
  <span><%= link_to user.username, user, class: "font-fam1 userpartialname" %></span>
  <% if current_user.admin? && !current_user?(@user) %>
  | <%= link_to "delete", user, method: :delete,
  data: { confirm: "You sure?" } %>
  <% end %>
  
  <% if user.verify == true%>
  <%= image_tag('Verification1.jpg', :class => "verify", "data-toggle"=>"modal", "data-placement"=>"top", "title"=>"Verified Account") %>
  <% else %>
  <% end %>
 </li>
  
 <li class="font-fam2"><%= user.bio %></li>
</ul>

</span>
<hr />
